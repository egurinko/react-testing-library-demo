{
  "extends": [
    "airbnb-typescript-prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "max-len": ["error", { "code": 100 }],
    "no-shadow": "off",
    "react-hooks/exhaustive-deps": "off",
    "import/no-cycle": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
